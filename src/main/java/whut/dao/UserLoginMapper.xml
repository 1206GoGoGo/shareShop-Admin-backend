<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="whut.dao.UserLoginDao">
  <resultMap id="BaseResultMap" type="whut.pojo.UserLoginLog">
    <id column="log_id" jdbcType="INTEGER" property="logId" />
    <result column="login_time" jdbcType="TIMESTAMP" property="loginTime" />
    <result column="login_ip" jdbcType="VARCHAR" property="loginIp" />
    <result column="login_type" jdbcType="TINYINT" property="loginType" />
    <result column="user_id" jdbcType="INTEGER" property="userId" />
  </resultMap>
  
   <select id="getLoginInfo" resultMap="BaseResultMap">
        SELECT * FROM user_login where username = #{username}
   </select>

</mapper>